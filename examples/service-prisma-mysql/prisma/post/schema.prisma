generator client {
  provider = "prisma-client-js"
  output   = env("CLIENT_OUTPUT")
}

datasource db {
  provider = ["sqlite", "mysql", "postgresql"]
  url      = env("DATABASE_URL")
}

model Post {
  authorId  Int?
  content   String?
  id        Int     @id @default(autoincrement())
  published Boolean @default(false)
  title     String
}
